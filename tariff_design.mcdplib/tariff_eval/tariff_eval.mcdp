dp {
    # From the tariff, lookup the expected purchase cost of energy, and the
    # revenue from the customers
    provides tariff [`tariff]

    requires purchase_cost_A [$]
    requires purchase_cost_B [$]
    requires revenue_A [$]
    requires revenue_B [$]
    requires fixed_cost_fracA [dimensionless]
    requires fixed_cost_fracB [dimensionless]

    implemented-by yaml resource("tariff_eval.dpc.yaml")
}
